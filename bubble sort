def bubblesort(array):
   n=len(array)
   for i in range(n):
       for j in range(0,n-i-1):
           if array[j]>array[j+1]:
               array[j],array[j+1]=array[j+1],array[j]
array=[-1,15,22,-4,16]
bubblesort(array)
print ("Sorted array is:")
for i in range(len(array)):
   print (array[i])


OUTPUT:-
Sorted array is:
-4
-1
15
16
22
